{
	"colors-enabled": true,
	"iam": {
		"policies": {
			"prefix": "",
			"suffix": "-generated",
			"version": "2012-10-17"
		}
	},
	"autoscaling": {
		"groups": {
			"override-launch-config-on-sync": false
		}
	},
	"route53": {
		"print-all-ignored": true
	},
	"s3": {
		"print-progress": true
	},
	"security": {
		"outbound-default-all-allowed": true,
		"subnet-files": [
			"security-groups/subnets.json"
		]
	},
	"ec2": {
		"instances": {
			"ignore-unmanaged": true,
			"default-image-id": null,
			"volume-mounting": {
				"base": "/dev/xvd",
				"start": "f",
				"end": "z"
			}
		}
	},
	"region": "us-east-1"
}
