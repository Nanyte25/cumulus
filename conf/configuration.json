{
	"colors-enabled": true,
	"iam": {
		"groups": {
			"directory": "iam/groups"
		},
		"policies": {
			"static": {
				"directory": "iam/policies/static"
			},
			"prefix": "",
			"suffix": "-generated",
			"templates": {
				"directory": "iam/policies/template"
			},
			"version": "2012-10-17"
		},
		"roles": {
			"directory": "iam/roles",
			"policy-document-directory": "iam/roles/policy-documents"
		},
		"users": {
			"directory": "iam/users"
		}
	},
	"autoscaling": {
		"groups": {
			"directory": "autoscaling/groups",
			"override-launch-config-on-sync": false
		},
		"policies": {
			"static": {
				"directory": "autoscaling/policies/static"
			},
			"templates": {
				"directory": "autoscaling/policies/templates"
			}
		}
	},
	"route53": {
		"print-all-ignored": true,
		"includes": {
			"directory": "route53/includes"
		},
		"zones": {
			"directory": "route53/zones"
		}
	},
	"s3": {
		"buckets": {
			"cors": {
				"directory": "s3/cors"
			},
			"directory": "s3/buckets",
			"policies": {
				"directory": "s3/policies"
			}
		},
		"print-progress": true
	},
	"security": {
		"groups": {
			"directory": "security/groups"
		},
		"subnets-file": "security/subnets.json",
		"outbound-default-all-allowed": true
	},
	"cloudfront": {
		"distributions": {
			"directory": "cloudfront/distributions"
		},
		"invalidations": {
			"directory": "cloudfront/invalidations"
		}
	},
	"elb": {
		"load-balancers": {
			"directory": "elb/load-balancers"
		},
		"listeners": {
			"directory": "elb/listeners"
		},
		"policies": {
			"directory": "elb/policies"
		}
	},
	"vpc": {
		"vpcs": {
			"directory": "vpc/vpcs"
		},
		"subnets": {
			"directory": "vpc/subnets"
		},
		"route-tables": {
			"directory": "vpc/route-tables"
		},
		"policies": {
			"directory": "vpc/policies"
		},
		"network-acls": {
			"directory": "vpc/network-acls"
		}
	},
	"kinesis": {
		"directory": "kinesis"
	},
	"sqs": {
		"queues": {
			"directory": "sqs/queues"
		},
		"policies": {
			"directory": "sqs/policies"
		}
	},
	"ec2": {
		"ebs": {
			"directory": "ec2/ebs"
		},
		"instances": {
			"directory": "ec2/instances",
			"ignore-unmanaged": true,
			"user-data-directory": "ec2/user-data-scripts",
			"default-image-id": null,
			"volume-mounting": {
				"base": "/dev/xvd",
				"start": "f",
				"end": "z"
			}

		}
	},
	"region": "us-east-1"
}
